	$(CC) -o CT3005 $<

CT3006: CT3006.c
	$(CC) -o CT3006 $<

CT3007: CT3007.c
	$(CC) -o CT3007 $<

CT3008: CT3008.c
	$(CC) -o CT3008 $<

